#include <iostream>
using namespace std;

int main() {
int n;
cin >>n;
int arr[n];
for(int i=0;i<n;i++)
{
	cin >> arr[i];
}
for(int i=0;i<n;i++)
{	
	for(int j=0;j<n-i-1;j++)
	{
	if(arr[j]>arr[j+1])
	{
	int temp= 0;
	temp = arr[j];
	arr[j]=arr[j+1];
	arr[j+1]=temp;
	}
	}
}
int mult = arr[n-2] * arr[n-1];
 cout<<mult;
 cout<<endl;

	return 0;
} // time complxity = n^2 //
