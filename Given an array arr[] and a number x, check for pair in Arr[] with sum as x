#include <iostream>
using namespace std;

int main() {
int x;int arr[10];int n;int i,j; int flag=0;
cout<<"enter the number whose sum will be equal to ";
cin>> x;
cout<<"enter the no of elements";
cin >> n;
arr[n];
for(i=0;i<n;i++)
{
    cin >> arr[i];
}
for(i=0;i<n-1;i++)
{
    for(j=i+1;j<i;j++)
    {
        
        if(arr[i]+arr[j]==x)
        {
            flag=1;
        }
    else
        {
            cout<<"pair not found";
        }
    if(flag==1)
    {
        cout<<i<<j;
        
        
    }

    }
    cout<< endl;
}
	return 0;
}
